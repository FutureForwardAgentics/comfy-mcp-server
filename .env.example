# ComfyUI Server Configuration
COMFY_URL=http://localhost:8188
COMFY_URL_EXTERNAL=http://localhost:8188
COMFY_WORKFLOW_JSON_FILE=/path/to/your/workflow.json

# Node IDs (will be auto-discovered if not set)
POS_PROMPT_NODE_ID=
NEG_PROMPT_NODE_ID=
FILEPATH_NODE_ID=
OUTPUT_NODE_ID=

# Output Configuration
OUTPUT_MODE=file
COMFY_WORKING_DIR=/path/to/save/images
COMFY_OUTPUT_DIR=/path/to/comfyui/output

# Optional: Ollama Configuration for Prompt Generation
OLLAMA_API_BASE=http://localhost:11434
PROMPT_LLM=llama2

# Optional: Debug Mode
DEBUG=false
